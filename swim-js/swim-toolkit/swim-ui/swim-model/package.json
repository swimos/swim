  "main": "./dist/swim-model.js",
  "module": "./dist/swim-model.mjs",
  "exports": {
    ".": {
      "import": "./dist/swim-model.mjs",
      "require": "./dist/swim-model.js",
      "types": "./dist/swim-model.d.ts"
